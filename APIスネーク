<script>
import AgencyAPI from '@/api/agencyApi';

export default {
  name: 'AgencyCard',
  
  props: {
    agencyCd: {
      type: String,
      required: true
    }
  },
  
  data() {
    return {
      agencyInfo: {                    // ã‚­ãƒ£ãƒ¡ãƒ«ã‚±ãƒ¼ã‚¹
        agencyCd: '',                  // ã‚­ãƒ£ãƒ¡ãƒ«ã‚±ãƒ¼ã‚¹
        agencyName: '',                // ã‚­ãƒ£ãƒ¡ãƒ«ã‚±ãƒ¼ã‚¹
        agencyEnName: '',              // ã‚­ãƒ£ãƒ¡ãƒ«ã‚±ãƒ¼ã‚¹
        phoneNumber: '',               // ã‚­ãƒ£ãƒ¡ãƒ«ã‚±ãƒ¼ã‚¹
        businessHours: '',             // ã‚­ãƒ£ãƒ¡ãƒ«ã‚±ãƒ¼ã‚¹
        businessHoursEn: '',           // ã‚­ãƒ£ãƒ¡ãƒ«ã‚±ãƒ¼ã‚¹
        medicalRabbitDealerFlag: false // ã‚­ãƒ£ãƒ¡ãƒ«ã‚±ãƒ¼ã‚¹
      },
      isLoading: false,                // ã‚­ãƒ£ãƒ¡ãƒ«ã‚±ãƒ¼ã‚¹
