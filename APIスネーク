  extends: [
    'eslint:recommended',
    '@vue/typescript/recommended'
  ],
  rules: {
    // ã‚­ãƒ£ãƒ¡ãƒ«ã‚±ãƒ¼ã‚¹ã®å¼·åˆ¶
    'camelcase': ['error', {
      properties: 'always',
      ignoreDestructuring: false,
      ignoreImports: false,
      ignoreGlobals: false
    }],
    
    // å‘½åè¦å‰‡ã®è©³ç´°è¨­å®š
